  "scripts": {
    "start": "yarn && styleguidist server & npm run watch:styles:types",
    "release": "yarn build:lib && semantic-release",
    "prebuild:lib": "mkdir -p lib",
    "build:src": "gulp build",
    "build:node": "NODE_ENV=production webpack --config webpack.server.js",
    "build:types": "tsc -p ./tsconfig.types.json",
    "build:docs:assests": "gulp dev:docs",
    "build:scss": "gulp build:scss",
    "build:cssModules": "gulp build:cssModules",
    "build:styles:types": "gulp build:styles:types",
    "build:lib": "gulp build:lib && yarn build:types",
    "build:cypressHelpers": "gulp build:cypressHelpers",
    "heroku-postbuild": "./scripts/heroku_deploy.sh",
    "watch:styles:types": "gulp watch:styles:types",
    "test": "jest --env=jsdom --maxWorkers=2",
    "test:update": "yarn test:snapshot:update",
    "test:snapshot:update": "yarn test -u",
    "test:percy": "yarn build:percy && percy-storybook --widths=320,1280",
    "build:percy": "NODE_ENV=development node --max-old-space-size=8192 ./node_modules/.bin/build-storybook -c ./storybook-config/.storybook-percy",
    "lint": "yarn lint:js && yarn lint:css && yarn prettier:check",
    "lint:js": "eslint --ext js --ext jsx --ext ts --ext tsx src",
    "lint:css": "stylelint src",
    "prettier:format": "prettier --write src/**/*.{js,jsx,ts,tsx} docs/src/**/*.{js,jsx,ts,tsx}",
    "prettier:check": "prettier --check src/**/*.{js,jsx,ts,tsx} docs/src/**/*.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -c ./storybook-config/.storybook --docs -p 6006 -s ./storybook-config/assets",
    "storybook:ci": "build-storybook --docs",
    "percy:storybook:start": "start-storybook -c ./storybook-config/.storybook-percy -s ./storybook-config/assets",
    "docs:storybook:build": " STORYBOOK_GOOGLE_ANALYTICS=\"$GOOGLE_ANALYTICS\" build-storybook -c ./storybook-config/.storybook -o ./recess-docs/components -s ./storybook-config/assets --quiet",
    "docs:storybook:start": "start-storybook --docs -p 6006 -s ./storybook-config/assets",
    "docs:css:start": "start-storybook -p 6007 -c ./storybook-config/.storybook-css -s ./storybook-config/assets",
    "docs:css:build": "STORYBOOK_GOOGLE_ANALYTICS=\"$GOOGLE_ANALYTICS\" build-storybook -c ./storybook-config/.storybook-css -o ./recess-docs/styles -s ./storybook-config/assets --quiet",
    "legacy-docs:styleguidest:start": "styleguidist server",
    "legacy-docs:styleguidest:build": "styleguidist build",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "danger": "danger ci -d ./dangerFiles/index.ts",
    "hygen": "hygen",
    "generator:help": "hygen component help",
    "component:new": "hygen component new",
    "component:icon": "hygen component icon",
    "story:new": "hygen component story",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "build:docs": "cross-env NODE_ENV=production webpack --config ./webpack.docs.js",
    "start:docs": "yarn run build:docs && cd docs && yarn start",
    "deploy:docs": "yarn build:docs && yarn docs:storybook:build && yarn docs:css:build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write src/**/*.{js,jsx,ts,tsx}",
      "git add"
    ]
  },
  "prettier": "@guildeducationinc/prettier-config",
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/testUtils/testSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/__tests__/*",
      "!src/**/*.scss.d.ts",
      "!src/**/stories/*"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/dangerFiles/**/__tests__/**/*.js?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    }
  },
  "description": "Guild Education's Style Guide and Component Library",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuildEducationInc/recess.git"
